<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>


	<package name="refund" namespace="/pages/refund" extends="global">
		<default-action-ref name="index" />
		<action name="*RefundAuditAction"
			class="com.zlebank.zplatform.manager.action.refund.RefundAuditAction"
			method="{1}">
			<result name="success">/pages/refund/refund_manager.jsp</result>
			<result name="showRefund">/pages/refund/refund_detail.jsp</result>
		</action>

	</package>


</struts>
