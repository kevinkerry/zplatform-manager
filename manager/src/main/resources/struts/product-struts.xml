<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="product" namespace="/pages/product" extends="global">
		<default-action-ref name="index" />
		<action name="*ProductAction"
			class="com.zlebank.zplatform.manager.action.merch.ProductAction"
			method="{1}">
			<result name="success">/pages/business/product_manager.jsp</result>
			<result name="cash_manager">/pages/business/cash_manager.jsp</result>

		</action>

	</package>

</struts>
